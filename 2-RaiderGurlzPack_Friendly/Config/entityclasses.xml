	<!-- Give melee Whiteriver raider gurlz necessary AI packages and interactions -->
	<append xpath="/entity_classes/entity_class[starts-with(@name, 'npcRaider') and starts-with(@extends, 'Raider')]/descendant::property[@name='Tags' and contains(@value, 'melee')]/parent::entity_class/descendant::property[@name='Faction' and @value='whiteriver']/parent::entity_class">

		<property name="IsEnemyEntity" value="false"/>
		<property name="LootListAlive" value="traderNPC"/>
		<property name="NPCID" value="spheretest"/> 
		<property name="AIPackages" value="NPCBasic, NPCMelee, NPCHired"/>

	</append>

	<!-- Give ranged Whiteriver raider gurlz necessary AI packages and interactions -->
